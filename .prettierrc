{
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": ["prettier-plugin-blade", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.blade.php",
            "vscodeLanguageIds": ["blade"],
            "options": {
                "parser": "blade"
            }
        }
    ]
}